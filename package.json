{
	"name": "issue-blog",
  "displayName": "Issue Blog",
  "author": "whyour",
  "license": "MIT",
  "keywords": ["issue", "blog", "github"],
  "icon": "https://image.whyour.cn/blogger.png",
	"description": "A vscode plugin to create a blog with issue",
	"version": "0.0.2",
	"publisher": "whyour",
	"repository": "https://github.com/whyour/issue-blog",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createIssue",
		"onCommand:extension.getIssues"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createIssue",
				"title": "Create Issue"
			},
			{
				"command": "extension.getIssues",
				"title": "Get Issues"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Use Issue Create Blog",
			"properties": {
				"issue.username": {
					"type": "string",
					"default": "",
					"description": "username",
					"scope": "window"
				},
				"issue.password": {
					"type": "string",
					"default": "",
					"description": "password",
					"scope": "window"
				},
				"issue.owner": {
					"type": "string",
					"default": "",
					"description": "owner",
					"scope": "window"
				},
				"issue.repo": {
					"type": "string",
					"default": "",
					"description": "repo",
					"scope": "window"
				},
				"issue.token": {
					"type": "string",
					"default": "",
					"description": "token",
					"scope": "window"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@octokit/rest": "^16.34.1"
	},
	"__metadata": {
		"id": "af5d7c94-6362-4e44-bab4-4c0b9e40d14f",
		"publisherDisplayName": "whyour",
		"publisherId": "01ad1390-d503-4956-a0da-160ce279ed86"
	}
}